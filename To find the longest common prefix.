#include <stdio.h>
#include <string.h>

void longestCommonPrefix(char strs[][100], int n) {
    char prefix[100];
    strcpy(prefix, strs[0]);

    for (int i = 1; i < n; i++) {
        int j = 0;
        while (prefix[j] && strs[i][j] && prefix[j] == strs[i][j])
            j++;
        prefix[j] = '\0';
    }

    printf("%s", prefix);
}

int main() {
    char strs[][100] = {"flower", "flow", "flight"};
    int n = 3;

    longestCommonPrefix(strs, n);
    return 0;
}
